<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane alignment="TOP_CENTER" prefHeight="200.0" prefWidth="266.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CalculatorController">
   <children>
      <Pane prefHeight="14.0" prefWidth="200.0" />
      <HBox prefHeight="32.0" prefWidth="249.0">
         <children>
            <Label prefHeight="17.0" prefWidth="100.0" text="Number 1:" />
            <TextField fx:id="number1TextField" alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="220.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="25.0" prefWidth="249.0">
         <children>
            <Label prefHeight="17.0" prefWidth="100.0" text="Number 2:" />
            <TextField fx:id="number2TextField" alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="218.0" />
         </children>
         <opaqueInsets>
            <Insets left="10.0" />
         </opaqueInsets>
      </HBox>
      <Pane prefHeight="12.0" prefWidth="200.0" />
      <GridPane alignment="BOTTOM_CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Sum:" />
            <TextField fx:id="sumTextField" editable="false" prefHeight="25.0" prefWidth="153.0" GridPane.columnIndex="1" />
            <Label prefHeight="17.0" prefWidth="18.0" text="Dif:" GridPane.rowIndex="1" />
            <TextField fx:id="difTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label prefHeight="17.0" prefWidth="59.0" text="Mult:" GridPane.rowIndex="2" />
            <TextField fx:id="multTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Div:" GridPane.rowIndex="3" />
            <TextField fx:id="divTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</FlowPane>
